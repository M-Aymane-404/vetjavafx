<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.vetjavafx.Controller.AddOwnerController">
    <children>
        <TextField fx:id="firstNameField" layoutX="150.0" layoutY="100.0" promptText="First Name" />
        <TextField fx:id="lastNameField" layoutX="150.0" layoutY="150.0" promptText="Last Name" />
        <TextField fx:id="addressField" layoutX="150.0" layoutY="200.0" promptText="Address" />
        <TextField fx:id="cityField" layoutX="150.0" layoutY="250.0" promptText="City" />
        <TextField fx:id="phoneField" layoutX="150.0" layoutY="300.0" promptText="Phone" />
        <Button fx:id="saveOwnerButton" text="Save Owner" layoutX="150.0" layoutY="350.0" onAction="#handleSaveOwnerButtonClick" />
    </children>
</AnchorPane>
